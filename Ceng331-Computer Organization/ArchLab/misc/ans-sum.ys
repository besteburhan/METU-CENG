#e2171395

.pos 0
    irmovq stack, %rsp
    irmovq ele1,%rdi
    call sum_list
    halt

.align 8
    ele1:
      .quad 0x00a
      .quad ele2
    ele2:
      .quad 0x0b0
      .quad ele3
    ele3:
      .quad 0xc00
      .quad 0

sum_list:
    xorq %rax,%rax
    andq %rdi,%rdi
    jne loop
    ret
loop:
    mrmovq (%rdi),%r9
    addq %r9,%rax
    mrmovq 8(%rdi),%rdi
    andq %rdi,%rdi
    jne loop
    ret
.pos 0x200
stack:
